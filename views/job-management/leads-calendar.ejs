<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span> Jobs</span><i class="fa fa-circle"></i></li>
                <li><span> Calendar</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div class="portlet light portlet-fit bordered calendar">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-calendar font-blue-hoki"></i>
                            <span class="caption-subject font-blue-hoki sbold uppercase"> Job Calendar</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <input type="hidden" id="apiUrl" value="<%= api.url %>"/>
                        <input type="hidden" id="token" value="<%= user.token %>"/>
                        <div class="row">
                            <div class="col-md-12 col-sm-12">
                                <div id="calendar" class="has-toolbar"> </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>