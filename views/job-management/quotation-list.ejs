<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span><a href="/leads">Leads</a></span><i class="fa fa-circle"></i></li>
                <li><span>Quotations</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered" id="quotations_portlet">
                    <div class="portlet-title">
                        <div class="caption font-blue-hoki">
                            <i class="fa fa-file-text-o font-blue-hoki"></i>
                            <span class="caption-subject bold uppercase"> Quotations </span>
                        </div>

                        <div class="dt-buttons pull-right">
                            <!-- <a class="dt-button buttons-print btn green btn-outline" href="/quotations/new"><span><i class="icon-plus"></i> Add Quotation</span></a> -->

                           <%- partial('partials/al/table-tools') %>
                        </div>
                    </div>

                     <div class="portlet-body">
                        <div class="dataTables_wrapper no-footer">
                            <div class="portlet box table-filters hide">
                                <form class="horizontal-form">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="col-md-2 col-sm-6">
                                                <label class="control-label">Customer</label>
                                                <select id="customerId" class="form-control select2-multiple select2-hidden-accessible" name="customerId">
                                                </select>
                                            </div>
                                            <div class="col-md-2 col-sm-6">
                                                <label class="control-label">Vendor</label>
                                                <select id="vendorId" class="form-control select2-multiple select2-hidden-accessible" name="vendorId">
                                                </select>
                                            </div>
                                            <div class="col-md-4 col-sm-6">
                                                <label class="control-label">Date Range</label>
                                                <div id="reportrange" class="btn default">
                                                    <i class="fa fa-calendar"></i> &nbsp;
                                                    <span> </span>
                                                    <b class="fa fa-angle-down"></b>
                                                </div>
                                            </div>
                                             <div class="col-md-2 col-sm-6 text-right">
                                                <label class="control-label">&nbsp;</label><br>
                                                <button id="filter-submit" class="btn green btn-outline">
                                                    <i class="fa fa-search"></i> Search
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>

                            <div class="portlet-body" id="tDiv">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="quotations_list">
                                    <thead>
                                        <tr class="uppercase">
                                            <th class="table-checkbox">
                                                <input type="checkbox" class="group-checkable" data-set="#quotations_list .checkboxes" /> </th>
                                            <th>Quote Id</th>
                                            <th> Lead Id</th>
                                            <th> Customer </th>
                                            <!-- <th> Name </th> -->
                                            <!-- <th> Mobile </th>
                                            <th> Address </th> -->
                                            <th> Amount </th>
                                        </tr>
                                    </thead>
                                    <tbody id="tableData"></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" id="apiUrl" value="<%= api.url %>"/>
<input type="hidden" id="token" value="<%= user.token %>"/>