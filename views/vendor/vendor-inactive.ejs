
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span>Vendors</span></li>
            </ul>
            <div class="page-toolbar">
                <div id="date-range" class="pull-right btn btn-sm">
                    <i class="icon-calendar"></i>&nbsp;
                    <span class="thin uppercase"></span>&nbsp;
                    <i class="fa fa-angle-down"></i>
                </div>
            </div>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered" id="vendors_portlet">
                    <div class="portlet-title">
                        <div class="caption font-blue-hoki">
                            <i class="fa fa-bullhorn font-blue-hoki"></i>
                            <span class="caption-subject bold uppercase"> Vendor List </span>
                        </div>
                        <div class="btn-group pull-right">
                            <a class="btn sbold green" href="/vendors/new" style="padding:6px 12px;margin-right:5px;"><span><i class="icon-plus"></i> Add Vendor</span></a>
                            <%- partial('partials/al/table-tools') %>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="dataTables_wrapper no-footer">
                            <div class="portlet-body" id="tDiv">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="vendors_list">
                                    <thead>
                                        <tr class="uppercase">
                                            <th class="table-checkbox">
                                                <input type="checkbox" class="group-checkable" data-set="#vendors_list .checkboxes" /> </th>
                                            <th> Mobile </th>
                                            <th> Name </th>
                                            <th> Email </th>
                                            <th> Address </th>
                                            <th> Status </th>
                                        </tr>
                                    </thead>
                                    <tbody id="tableData"></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="apiUrl" value="<%= api.url %>"/>
<input type="hidden" id="token" value="<%= user.token %>"/>
<input type="hidden" id="status" value="<%= status %>">
