<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span>Support</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered" id="support_portlet">
                    <div class="portlet-title">
                        <div class="caption font-blue-hoki">
                            <i class="fa fa-paper-plane font-blue-hoki"></i>
                            <span class="caption-subject bold uppercase"> Ticket List </span>
                        </div>

                        <div class="dt-buttons pull-right">
                            <a class="dt-button buttons-print btn green btn-outline" href="/support/new"><span><i class="icon-plus"></i> Add Ticket</span></a>

                           <%- partial('partials/al/table-tools') %>
                        </div>
                    </div>

                     <div class="portlet-body">
                        <div class="dataTables_wrapper no-footer">
                            <div class="portlet box table-filters hide">
                                <form class="horizontal-form">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="col-md-2 col-sm-6">
                                                <label class="control-label">Priority</label>
                                                <select class="form-control" name="priority" id="priority">
                                                    <option value="-1" selected="true">All Priorities</option>
                                                    <option value="low">Low</option>
                                                    <option value="medium">Medium</option>
                                                    <option value="high">High</option>
                                                </select>
                                            </div>
                                            <div class="col-md-2 col-sm-6">
                                                <label class="control-label">Type</label>
                                                <select class="form-control" name="type" id="type">
                                                    <option value="-1" selected="true">All Kinds</option>
                                                    <option value="bug">Bug</option>
                                                    <option value="enhanancement">Enhanancement</option>
                                                    <option value="task">Task</option>
                                                    <option value="proposal">Proposal</option>
                                                </select>
                                            </div>
                                             <div class="col-md-2 col-sm-6 text-right">
                                                <label class="control-label">&nbsp;</label><br>
                                                <button id="filter-submit" class="btn green btn-outline">
                                                    <i class="fa fa-search"></i> Search
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>

                            <div class="portlet-body" id="tDiv">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="support_list">
                                    <thead>
                                        <tr class="uppercase">
                                            <th class="table-checkbox">
                                                <input type="checkbox" class="group-checkable" data-set="#support_list .checkboxes" /> </th>
                                            <th> Ticket Id </th>
                                            <th> Title </th>
                                            <th> Customer Name </th>
                                            <th> Service Name </th>
                                            <th> Due Date</th>
                                            <th> Assigned To</th>
                                            <th> Priority</th>
                                            <th> Status</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tableData"></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="apiUrl" value="<%= api.url %>"/>
<input type="hidden" id="token" value="<%= user.token %>"/>

<%- partial('partials/modal/assignTicket') %>
