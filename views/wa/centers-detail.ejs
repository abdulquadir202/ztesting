<link href="/assets/global/plugins/cubeportfolio/css/cubeportfolio.css" rel="stylesheet" type="text/css" />
<link href="/assets/pages/css/portfolio.min.css" rel="stylesheet" type="text/css" />
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span><a href="/wa/centers">Centers</a></span><i class="fa fa-circle"></i></li>
                <li><span>Details</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-dot-circle-o font-blue-hoki"></i>
                            <span class="caption-subject font-blue-hoki bold uppercase">Centers Detail</span>
                            <!-- <span class="caption-helper">some info...</span> -->
                        </div>
                        <div class="dt-buttons pull-right">
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-edit"></i> Edit</span></a>
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-trash"></i> Delete</span></a>
                            <!-- <a href="" class="collapse"> </a>
                            <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                            <a href="" class="reload"> </a>
                            <a href=""> <i class="fa fa-edit"></i> </a>
                            <a href=""> <i class="fa fa-trash"></i> </a> -->
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        <input type="hidden" id="centerId" value="<%= centerId %>">
                        <input type="hidden" id="url" value="<%= api.url %>/api/wa/center/<%= centerId %>?token=<%= user.token %>">

                        <form class="form-horizontal" role="form">
                            <div class="form-body">
                                
                                <div class="form-group">
                                    <label class="control-label col-md-3">Center Name:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayCenterName"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Center Type:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayCenterType"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Parent Center:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayCenter"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Inaugurated By:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayInauguratedBy"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Address:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayAddress"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Location:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayLocation"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Mobile:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayMobile"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Phone Number:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayPhoneNumber"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Fax Number:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayFaxNumber"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Email:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayEmail"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Website Url:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayWebsiteUrl"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Registered Name:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayRegisteredName"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Registration Number:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayRegistrationNumber"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Inauguration Date:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayInaugurationDate"></p>
                                    </div>
                                </div>
                           
                                <div class="form-group">
                                    <label class="control-label col-md-3">Subsidiary Canteen Id:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displaySubsidiaryCanteenId"></p>
                                    </div>
                                </div>
                                    
                            </div>
                            
                        </form>
                        <!-- END FORM-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

