<link href="/assets/global/plugins/cubeportfolio/css/cubeportfolio.css" rel="stylesheet" type="text/css" />
<link href="/assets/pages/css/portfolio.min.css" rel="stylesheet" type="text/css" />
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span><a href="/wa/members">Members</a></span><i class="fa fa-circle"></i></li>
                <li><span>Details</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-users font-blue-hoki"></i>
                            <span class="caption-subject font-blue-hoki bold uppercase">Members Detail</span>
                            <!-- <span class="caption-helper">some info...</span> -->
                        </div>
                        <div class="dt-buttons pull-right">
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-edit"></i> Edit</span></a>
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-trash"></i> Delete</span></a>
                            <!-- <a href="" class="collapse"> </a>
                            <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                            <a href="" class="reload"> </a>
                            <a href=""> <i class="fa fa-edit"></i> </a>
                            <a href=""> <i class="fa fa-trash"></i> </a> -->
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        <input type="hidden" id="memberId" value="<%= memberId %>">
                        <input type="hidden" id="url" value="<%= api.url %>/api/wa/member/<%= memberId %>?token=<%= user.token %>">

                        <form class="form-horizontal" role="form">
                            <div class="form-body">

                                
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Name:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayName"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Picture:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayPicture"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Rank:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displaySpouseRank"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Spouse Name:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displaySpouseName"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Address:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayAddress"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Mobile:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayMobile"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Spouse Registration Number:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displaySpouseRegistrationNumber"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Spouse Job Location:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displaySpouseJobLocation"></p>
                                            </div>
                                        </div>
                                   
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Date Of Birth:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayDateOfBirth"></p>
                                            </div>
                                        </div>
                                    
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Start Date:</label>
                                            <div class="col-md-9">
                                                <p class="form-control-static" id="displayStartDate"></p>
                                            </div>
                                        </div>
                                    
                                </div>
                            </div>
                            
                        </form>
                        <!-- END FORM-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

