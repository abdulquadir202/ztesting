<link href="/assets/global/plugins/cubeportfolio/css/cubeportfolio.css" rel="stylesheet" type="text/css" />
<link href="/assets/pages/css/portfolio.min.css" rel="stylesheet" type="text/css" />
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li><span><a href="/dashboard">Home</a></span><i class="fa fa-circle"></i></li>
                <li><span><a href="/wa/employees">Employees</a></span><i class="fa fa-circle"></i></li>
                <li><span>Details</span></li>
            </ul>
        </div>
        <h1 class="page-title"></h1>
        <div class="row">
            <div class="col-md-12">
                <div id="form-alerts"> </div>
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-users font-blue-hoki"></i>
                            <span class="caption-subject font-blue-hoki bold uppercase">Employees Detail</span>
                        </div>
                        <div class="dt-buttons pull-right">
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-edit"></i> Edit</span></a>
                            <a class="dt-button buttons-print btn blue-hoki btn-outline" href="javascript:;"><span><i class="fa fa-trash"></i> Delete</span></a>
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        <input type="hidden" id="employeeId" value="<%= employeeId %>">
                        <input type="hidden" id="url" value="<%= api.url %>/api/employee/<%= employeeId %>?token=<%= user.token %>">

                        <form class="form-horizontal" role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="control-label col-md-3">Name:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayName"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Mobile:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayMobile"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Email:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayEmail"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Start Date:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayStartDate"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Designation:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayDesignation"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Department:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayDepartment"></p>
                                    </div>
                                </div>
                            
                                <div class="form-group">
                                    <label class="control-label col-md-3">Employment Type:</label>
                                    <div class="col-md-9">
                                        <p class="form-control-static" id="displayEmploymentType"></p>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="/js/onground/contacts/employees/employee-detail.js"></script>
<script src="/assets/global/plugins/cubeportfolio/js/jquery.cubeportfolio.min.js" type="text/javascript"></script>
<script src="/assets/pages/scripts/portfolio-1.min.js" type="text/javascript"></script>